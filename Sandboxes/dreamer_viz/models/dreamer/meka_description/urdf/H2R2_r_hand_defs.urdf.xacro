                    <mesh filename="${modelPath}/meka_description/meshes/${MESH_COLL}/H2R3/H2R3_finger_knuckle2.stl" />
                </geometry>
            </collision>
        </link>

        <gazebo reference="right_pointer_finger_pip">
            <mu1>1</mu1>
            <mu2>1</mu2>
        </gazebo>

        <joint name="right_pointer_finger_dip" type="${RIGHT_HAND_JOINT_TYPE}">
