    <xacro:macro name="m3_r_hand" params="side parent reflect *origin">
        <m3_r_palm side="${side}" reflect="${reflect}" parent="${parent}">
            <insert_block name="origin" />
        </m3_r_palm>
    </xacro:macro>
</robot>
