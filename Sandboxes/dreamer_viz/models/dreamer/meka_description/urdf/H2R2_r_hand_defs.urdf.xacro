            <axis xyz="0 1 0" />
            <origin xyz="0 -0.028 -0.101" rpy="0 0 0" />
            <parent link="palm" />
            <child link="right_pinky_mcp"/>
            <limit effort="100" velocity="${50*M_PI/180}" lower="${-7.072*M_PI/180}" upper="${37.072*M_PI/180}" />
