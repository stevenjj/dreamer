                <geometry name="right_middle_finger_pip_link_collision">
                    <mesh filename="${modelPath}/meka_description/meshes/${MESH_COLL}/H2R3/H2R3_finger_knuckle2.stl" />
                </geometry>
            </collision>
        </link>

        <gazebo reference="right_middle_finger_pip_link">
            <mu1>1</mu1>
            <mu2>1</mu2>
        </gazebo>

